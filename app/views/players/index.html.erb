<div>
  <h1 class="font-bold text-4xl">Players#index</h1>
  <p>
    <% @players.each do |player| %>
      <%= image_tag player.photo %>
      <%= player.name %>
      <%= t("activerecord.attributes.player.position_options.#{player.position}") %>
      <%= player.club.name %>
    <% end %>
  </p>
</div>
